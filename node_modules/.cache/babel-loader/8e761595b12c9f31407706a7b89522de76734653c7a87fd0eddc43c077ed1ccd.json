{"ast":null,"code":"export default function distanceToPointSquared(p1, p2) {\n  if (p1.length !== p2.length) {\n    throw Error('Both points should have the same dimensionality');\n  }\n  const [x1, y1, z1 = 0] = p1;\n  const [x2, y2, z2 = 0] = p2;\n  return Math.pow(x1 - x2, 2) + Math.pow(y1 - y2, 2) + Math.pow(z1 - z2, 2);\n}","map":{"version":3,"names":["distanceToPointSquared","p1","p2","length","Error","x1","y1","z1","x2","y2","z2","Math","pow"],"sources":["../../../../../src/utilities/math/point/distanceToPointSquared.ts"],"sourcesContent":[null],"mappings":"AAWA,eAAc,SAAUA,sBAAsBA,CAACC,EAAS,EAAEC,EAAS;EACjE,IAAID,EAAE,CAACE,MAAM,KAAKD,EAAE,CAACC,MAAM,EAAE;IAC3B,MAAMC,KAAK,CAAC,iDAAiD,CAAC;;EAGhE,MAAM,CAACC,EAAE,EAAEC,EAAE,EAAEC,EAAE,GAAG,CAAC,CAAC,GAAGN,EAAE;EAC3B,MAAM,CAACO,EAAE,EAAEC,EAAE,EAAEC,EAAE,GAAG,CAAC,CAAC,GAAGR,EAAE;EAE3B,OAAOS,IAAI,CAACC,GAAG,CAACP,EAAE,GAAGG,EAAE,EAAE,CAAC,CAAC,GAAGG,IAAI,CAACC,GAAG,CAACN,EAAE,GAAGG,EAAE,EAAE,CAAC,CAAC,GAAGE,IAAI,CAACC,GAAG,CAACL,EAAE,GAAGG,EAAE,EAAE,CAAC,CAAC;AAC3E"},"metadata":{},"sourceType":"module","externalDependencies":[]}