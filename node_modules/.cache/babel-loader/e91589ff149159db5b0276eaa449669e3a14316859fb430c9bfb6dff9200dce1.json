{"ast":null,"code":"export default function calculateAreaOfPoints(points) {\n  const n = points.length;\n  let area = 0.0;\n  let j = n - 1;\n  for (let i = 0; i < n; i++) {\n    area += (points[j][0] + points[i][0]) * (points[j][1] - points[i][1]);\n    j = i;\n  }\n  return Math.abs(area / 2.0);\n}","map":{"version":3,"names":["calculateAreaOfPoints","points","n","length","area","j","i","Math","abs"],"sources":["../../../../../src/utilities/math/polyline/calculateAreaOfPoints.ts"],"sourcesContent":[null],"mappings":"AASA,eAAc,SAAUA,qBAAqBA,CAACC,MAAsB;EAElE,MAAMC,CAAC,GAAGD,MAAM,CAACE,MAAM;EACvB,IAAIC,IAAI,GAAG,GAAG;EACd,IAAIC,CAAC,GAAGH,CAAC,GAAG,CAAC;EAEb,KAAK,IAAII,CAAC,GAAG,CAAC,EAAEA,CAAC,GAAGJ,CAAC,EAAEI,CAAC,EAAE,EAAE;IAC1BF,IAAI,IAAI,CAACH,MAAM,CAACI,CAAC,CAAC,CAAC,CAAC,CAAC,GAAGJ,MAAM,CAACK,CAAC,CAAC,CAAC,CAAC,CAAC,KAAKL,MAAM,CAACI,CAAC,CAAC,CAAC,CAAC,CAAC,GAAGJ,MAAM,CAACK,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACrED,CAAC,GAAGC,CAAC;;EAIP,OAAOC,IAAI,CAACC,GAAG,CAACJ,IAAI,GAAG,GAAG,CAAC;AAC7B"},"metadata":{},"sourceType":"module","externalDependencies":[]}